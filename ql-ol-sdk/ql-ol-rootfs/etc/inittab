# /etc/inittab: init(8) configuration.
# $Id: inittab,v 1.91 2002/01/25 13:35:21 miquels Exp $

# The default runlevel.
id:5:initdefault:

# Boot-time system configuration/initialization script.
# This is run first except when booting in emergency (-b) mode.
si::sysinit:/etc/init.d/rcS

# What to do in single-user mode.
~~:S:wait:/sbin/sulogin

#Set the storage daemons to respawn
# NOTE: These entries should be above the rc
#       run level entries with wait below.
#rmts:2345:respawn:/sbin/rmt_storage
tftp:2345:respawn:/sbin/tftp_server
scrb:2345:respawn:/sbin/fs-scrub-daemon

# /etc/init.d executes the S and K scripts upon change
# of runlevel.
#
# Runlevel 0 is halt.
# Runlevel 1 is single-user.
# Runlevels 2-5 are multi-user.
# Runlevel 6 is reboot.

l0:0:wait:/etc/init.d/rc 0
l1:1:wait:/etc/init.d/rc 1
l2:2:wait:/etc/init.d/rc 2
l3:3:wait:/etc/init.d/rc 3
l4:4:wait:/etc/init.d/rc 4
l5:5:wait:/etc/init.d/rc 5
l6:6:wait:/etc/init.d/rc 6

# Normally not reached, but fallthrough in case of emergency.
z6:6:respawn:/sbin/sulogin

#runs reboot daemon
#rb:5:respawn:/sbin/reboot-daemon

# Clean up any session file from a previous boot before starting mbimd
m1:5:wait:rm -f /var/run/qbi_session_active
m2:5:respawn:/usr/bin/mbimd
m3:5:once:/sbin/usb/compositions/quec_mbim_check

atcp:5:respawn:/data/QuecOpen/atcpipe
open:5:respawn:/etc/init.d/QuecOpen.sh
dual:5:respawn:/data/example_dual_apn

S:2345:respawn:/sbin/getty -L ttyHSL0 115200 console

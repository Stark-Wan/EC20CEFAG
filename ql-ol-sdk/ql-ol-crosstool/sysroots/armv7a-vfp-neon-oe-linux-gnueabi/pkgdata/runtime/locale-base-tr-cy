RDEPENDS_locale-base-tr-cy: localedef glibc-localedata-tr-cy glibc-charmap-utf-8
RPROVIDES_locale-base-tr-cy:  virtual-locale-tr-cy virtual-locale-tr
SECTION: base
PKG_locale-base-tr-cy: locale-base-tr-cy
ALLOW_EMPTY_locale-base-tr-cy: 1
pkg_postinst_locale-base-tr-cy: #!/bin/sh\n\nif [ "x$D" != "x" ]; then\n\texit 1\nfi\n\nrm -rf /tmp/locale/usr/lib/locale\nmkdir -p /tmp/locale/usr/lib/locale\nif [ -f /usr/lib/locale/locale-archive ]; then\n        cp /usr/lib/locale/locale-archive /tmp/locale/usr/lib/locale/\nfi\nlocaledef --inputfile=/usr/share/i18n/locales/tr_CY --charmap=UTF-8 --prefix=/tmp/locale tr_CY\nmkdir -p /usr/lib/locale/\nmv /tmp/locale/usr/lib/locale/locale-archive /usr/lib/locale/\nrm -rf /tmp/locale/usr/lib/locale\n
pkg_postrm_locale-base-tr-cy: #!/bin/sh\n\nrm -rf /tmp/locale/usr/lib/locale\nmkdir -p /tmp/locale/usr/lib/locale\nif [ -f /usr/lib/locale/locale-archive ]; then\n\tcp /usr/lib/locale/locale-archive /tmp/locale/usr/lib/locale/\nfi\nlocaledef --delete-from-archive --inputfile=/usr/share/locales/tr_CY --charmap=UTF-8 --prefix=/tmp/locale tr_CY\nmv /tmp/locale/usr/lib/locale/locale-archive /usr/lib/locale/\nrm -rf /tmp/locale/usr/lib/locale\n

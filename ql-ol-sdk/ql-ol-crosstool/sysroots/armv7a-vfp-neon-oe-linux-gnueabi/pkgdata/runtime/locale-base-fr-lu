RDEPENDS_locale-base-fr-lu: localedef glibc-localedata-fr-lu glibc-charmap-utf-8
RPROVIDES_locale-base-fr-lu:  virtual-locale-fr-lu virtual-locale-fr
SECTION: base
PKG_locale-base-fr-lu: locale-base-fr-lu
ALLOW_EMPTY_locale-base-fr-lu: 1
pkg_postinst_locale-base-fr-lu: #!/bin/sh\n\nif [ "x$D" != "x" ]; then\n\texit 1\nfi\n\nrm -rf /tmp/locale/usr/lib/locale\nmkdir -p /tmp/locale/usr/lib/locale\nif [ -f /usr/lib/locale/locale-archive ]; then\n        cp /usr/lib/locale/locale-archive /tmp/locale/usr/lib/locale/\nfi\nlocaledef --inputfile=/usr/share/i18n/locales/fr_LU --charmap=UTF-8 --prefix=/tmp/locale fr_LU\nmkdir -p /usr/lib/locale/\nmv /tmp/locale/usr/lib/locale/locale-archive /usr/lib/locale/\nrm -rf /tmp/locale/usr/lib/locale\n
pkg_postrm_locale-base-fr-lu: #!/bin/sh\n\nrm -rf /tmp/locale/usr/lib/locale\nmkdir -p /tmp/locale/usr/lib/locale\nif [ -f /usr/lib/locale/locale-archive ]; then\n\tcp /usr/lib/locale/locale-archive /tmp/locale/usr/lib/locale/\nfi\nlocaledef --delete-from-archive --inputfile=/usr/share/locales/fr_LU --charmap=UTF-8 --prefix=/tmp/locale fr_LU\nmv /tmp/locale/usr/lib/locale/locale-archive /usr/lib/locale/\nrm -rf /tmp/locale/usr/lib/locale\n

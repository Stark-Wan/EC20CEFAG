RDEPENDS_locale-base-ru-ua: glibc-localedata-ru-ua localedef glibc-charmap-utf-8
RPROVIDES_locale-base-ru-ua:  virtual-locale-ru-ua virtual-locale-ru
SECTION: base
PKG_locale-base-ru-ua: locale-base-ru-ua
ALLOW_EMPTY_locale-base-ru-ua: 1
pkg_postinst_locale-base-ru-ua: #!/bin/sh\n\nif [ "x$D" != "x" ]; then\n\texit 1\nfi\n\nrm -rf /tmp/locale/usr/lib/locale\nmkdir -p /tmp/locale/usr/lib/locale\nif [ -f /usr/lib/locale/locale-archive ]; then\n        cp /usr/lib/locale/locale-archive /tmp/locale/usr/lib/locale/\nfi\nlocaledef --inputfile=/usr/share/i18n/locales/ru_UA --charmap=UTF-8 --prefix=/tmp/locale ru_UA\nmkdir -p /usr/lib/locale/\nmv /tmp/locale/usr/lib/locale/locale-archive /usr/lib/locale/\nrm -rf /tmp/locale/usr/lib/locale\n
pkg_postrm_locale-base-ru-ua: #!/bin/sh\n\nrm -rf /tmp/locale/usr/lib/locale\nmkdir -p /tmp/locale/usr/lib/locale\nif [ -f /usr/lib/locale/locale-archive ]; then\n\tcp /usr/lib/locale/locale-archive /tmp/locale/usr/lib/locale/\nfi\nlocaledef --delete-from-archive --inputfile=/usr/share/locales/ru_UA --charmap=UTF-8 --prefix=/tmp/locale ru_UA\nmv /tmp/locale/usr/lib/locale/locale-archive /usr/lib/locale/\nrm -rf /tmp/locale/usr/lib/locale\n

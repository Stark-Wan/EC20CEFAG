RDEPENDS_locale-base-mt-mt: glibc-localedata-mt-mt localedef glibc-charmap-utf-8
RPROVIDES_locale-base-mt-mt:  virtual-locale-mt-mt virtual-locale-mt
SECTION: base
PKG_locale-base-mt-mt: locale-base-mt-mt
ALLOW_EMPTY_locale-base-mt-mt: 1
pkg_postinst_locale-base-mt-mt: #!/bin/sh\n\nif [ "x$D" != "x" ]; then\n\texit 1\nfi\n\nrm -rf /tmp/locale/usr/lib/locale\nmkdir -p /tmp/locale/usr/lib/locale\nif [ -f /usr/lib/locale/locale-archive ]; then\n        cp /usr/lib/locale/locale-archive /tmp/locale/usr/lib/locale/\nfi\nlocaledef --inputfile=/usr/share/i18n/locales/mt_MT --charmap=UTF-8 --prefix=/tmp/locale mt_MT\nmkdir -p /usr/lib/locale/\nmv /tmp/locale/usr/lib/locale/locale-archive /usr/lib/locale/\nrm -rf /tmp/locale/usr/lib/locale\n
pkg_postrm_locale-base-mt-mt: #!/bin/sh\n\nrm -rf /tmp/locale/usr/lib/locale\nmkdir -p /tmp/locale/usr/lib/locale\nif [ -f /usr/lib/locale/locale-archive ]; then\n\tcp /usr/lib/locale/locale-archive /tmp/locale/usr/lib/locale/\nfi\nlocaledef --delete-from-archive --inputfile=/usr/share/locales/mt_MT --charmap=UTF-8 --prefix=/tmp/locale mt_MT\nmv /tmp/locale/usr/lib/locale/locale-archive /usr/lib/locale/\nrm -rf /tmp/locale/usr/lib/locale\n

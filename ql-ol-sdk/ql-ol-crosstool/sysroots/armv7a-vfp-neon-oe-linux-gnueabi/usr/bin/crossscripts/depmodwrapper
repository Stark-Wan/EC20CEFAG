#!/bin/sh
# Expected to be called as: depmodwrapper -a KERNEL_VERSION
if [ "$1" != "-a" -o "$2" != "-b" ]; then
    echo "Usage: depmodwrapper -a -b rootfs KERNEL_VERSION" >&2
    exit 1
fi
if [ ! -r /home/q/anisa_nvme/anisa_test/Qualcomm/MDM9x07/OpenLinux/LE.1.0.c3/Customers/R06_b1_BYD/apps_proc/oe-core/build/tmp-glibc/sysroots/mdm9607-perf/pkgdata/kernel-depmod/kernel-abiversion ]; then
    echo "Unable to read: /home/q/anisa_nvme/anisa_test/Qualcomm/MDM9x07/OpenLinux/LE.1.0.c3/Customers/R06_b1_BYD/apps_proc/oe-core/build/tmp-glibc/sysroots/mdm9607-perf/pkgdata/kernel-depmod/kernel-abiversion" >&2
else
    kernelabi=$(cat /home/q/anisa_nvme/anisa_test/Qualcomm/MDM9x07/OpenLinux/LE.1.0.c3/Customers/R06_b1_BYD/apps_proc/oe-core/build/tmp-glibc/sysroots/mdm9607-perf/pkgdata/kernel-depmod/kernel-abiversion)
    if [ "$kernelabi" != "$4" ]; then
        echo "Error: Kernel version $4 does not match kernel-abiversion ($kernelabi)" >&2
        exit 1
    fi
fi

if [ ! -r /home/q/anisa_nvme/anisa_test/Qualcomm/MDM9x07/OpenLinux/LE.1.0.c3/Customers/R06_b1_BYD/apps_proc/oe-core/build/tmp-glibc/sysroots/mdm9607-perf/pkgdata/kernel-depmod/System.map-$4 ]; then
    echo "Unable to read: /home/q/anisa_nvme/anisa_test/Qualcomm/MDM9x07/OpenLinux/LE.1.0.c3/Customers/R06_b1_BYD/apps_proc/oe-core/build/tmp-glibc/sysroots/mdm9607-perf/pkgdata/kernel-depmod/System.map-$4" >&2
    exec env depmod "$1" "$2" "$3" "$4"
else
    exec env depmod "$1" "$2" "$3" -F "/home/q/anisa_nvme/anisa_test/Qualcomm/MDM9x07/OpenLinux/LE.1.0.c3/Customers/R06_b1_BYD/apps_proc/oe-core/build/tmp-glibc/sysroots/mdm9607-perf/pkgdata/kernel-depmod/System.map-$4" "$4"
fi
